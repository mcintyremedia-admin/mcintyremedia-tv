<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="service.network.tunnel" name="Network Tunnel" version="0.0.5" provider-name="McIntyreMedia|JC">
    <requires>
        <import addon="xbmc.metadata" version="1.0"/>
        <import addon="xbmc.addon" version="12.0.0"/>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.beautifulsoup" version="3.2.1"/>
        <import addon="script.module.mechanize" version="0.2.6"/>
    </requires>
    <extension point="xbmc.service" library="default.py" start="login"/>
    <extension point="xbmc.addon.metadata" language="english">
        <summary lang="en">Network tunnel</summary>
        <description lang="en">Network tunnel</description>
        <disclaimer lang="en">Please check if tunnel access is allowed in your local jurisdiction</disclaimer>
        <platform>linux</platform>
        <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 2013</license>
        <website>http://www.mcintyremedia.tv</website>
        <forum></forum>
        <source></source>
        <email>admin@mcintyremedia.tv</email>
    </extension>
</addon>

<addon id="plugin.audio.jointone" name="JointOne" version="1.0.2" provider-name="McIntyreMedia">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Joint One Radio</summary>
    <description lang="en">Enjoy Joint One's unique music mix</description>
    <disclaimer lang="en"></disclaimer>
    <language></language>
    <platform>all</platform>
    <email>admin@mcintyremedia.tv</email>
  </extension>
</addon>
</addons>
